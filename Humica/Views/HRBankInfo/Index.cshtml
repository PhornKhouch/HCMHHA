
<script type="text/javascript">
    $('#linkFavoriteList').hide();

    $(document).ready(function () {
        $('#_btnExportAction').click(function () {            
            window.location = "@Humica.EF.Models.SY.SYUrl.getBaseUrl()" + "@ViewData[Humica.EF.Models.SY.SYConstant.CURRENT_URL].ToString()" + "ExportTo?id=" + pcScrollingMaterial1.GetActiveTab().index;
        });
    });
</script>

@model   Humica.Logic.HRS.MDBankInfor

@Html.DevExpress().PageControl(
    settings =>
    {
        settings.Name = "pcScrollingMaterial1";
        settings.Width = Unit.Percentage(100);

        settings.EnableTabScrolling = true;
        settings.TabAlign = TabAlign.Justify;

        settings.Styles.Tab.CssClass = "scrolling_Tab";

        settings.TabPages.Add(Humica.EF.Models.SY.SYSettings.getLabel("Bank")).SetContent(() =>
        {
            @Html.RenderPartial("GridItemBanks", Model.ListBank);
        });
        settings.TabPages.Add(Humica.EF.Models.SY.SYSettings.getLabel("BankBranch")).SetContent(() =>
        {
            @Html.RenderPartial("GridItemBankBranchs", Model.ListBankBranch);
        });
        settings.TabPages.Add(Humica.EF.Models.SY.SYSettings.getLabel("Bank Account")).SetContent(() =>
        {
              @Html.RenderPartial("GridItemBankAcc", Model.ListBankAccount);
        });
        settings.TabPages.Add(Humica.EF.Models.SY.SYSettings.getLabel("Bank Service Fee")).SetContent(() =>
        {
            @Html.RenderPartial("GridItemBankFee", Model.ListBankFee);
        });
    }).GetHtml()


