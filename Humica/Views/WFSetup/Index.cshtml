@model   Humica.Logic.MD.ExWorkFlowPreference



@{
    if (Model.ScreenID != null)
    {
        @Html.DevExpress().PageControl(
             settings =>
             {
                 settings.Name = "pcScrollingMaterial1";
                 settings.Width = Unit.Percentage(100);

                 settings.EnableTabScrolling = true;
                 settings.TabAlign = TabAlign.Justify;

                 settings.Styles.Tab.CssClass = "scrolling_Tab";

                 settings.TabPages.Add(Humica.EF.Models.SY.SYSettings.getLabel("WorkFlow")).SetContent(() =>
                 {
                     @Html.RenderPartial("GridItems1", Model.ListWF);
                 });
                 settings.TabPages.Add(Humica.EF.Models.SY.SYSettings.getLabel("ApproverByBranch")).SetContent(() =>
                 {
                     @Html.RenderPartial("GridItems2", Model.ListWFApprover);
                 });
                 settings.TabPages.Add(Humica.EF.Models.SY.SYSettings.getLabel("WorkFlowItem")).SetContent(() =>
                 {
                     @Html.RenderPartial("GridItems4", Model.ListWFItem);
                 });
                 settings.TabPages.Add(Humica.EF.Models.SY.SYSettings.getLabel("Salary Approver")).SetContent(() =>
                 {
                     @Html.RenderPartial("GridItems5", Model.ListSalaryApprover);
                 });
             }).GetHtml();

        }
    }
