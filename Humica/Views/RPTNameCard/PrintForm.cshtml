@Html.DevExpress().DocumentViewer(
settings =>
{
    settings.Name = "DocumentViewer";
    settings.Report = Model;
    settings.SettingsSplitter.DocumentMapCollapsed = true;
    settings.CallbackRouteValues = new { Controller = "RPTNameCard", Action = "DocumentViewerPartial",id = ViewData[Humica.EF.SYSConstant.PARAM_ID].ToString() };
    settings.ExportRouteValues = new { Controller = "RPTNameCard", Action = "DocumentViewerExportTo" , id = ViewData[Humica.EF.SYSConstant.PARAM_ID].ToString() };
    settings.ToolbarMode = DevExpress.XtraReports.Web.DocumentViewer.DocumentViewerToolbarMode.StandardToolbar;
}).GetHtml()
