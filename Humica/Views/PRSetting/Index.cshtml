<script type="text/javascript">
    var ss;
    var selectedIDs = "";
    var isvisible = true;
    $(document).ready(function () {

        $('#_btnSave').click(function () {
            $("form").submit();
        });
    });
    function chkIsTax_CheckedChanged(s, e) {
        var _bool = false;
        _bool = s.GetChecked();
        txtSeniorityException.SetEnabled(_bool);
    }
    function Init()
    {
        txtSeniorityException.SetEnabled(chkIsTax.GetChecked());
