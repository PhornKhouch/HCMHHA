  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>select 
	S.EmpCode,S.EmployeName,S.AssetCode,S.AssignDate,S.Remark,R.AssetReference,EMP.Phone1,HRSection.Description AS sect,
	HRDepartment.Description DEPT,HRPosition.Description Position,HRLocation.Description LOCT,
	R.UsefulLifeYear,R.Model,R.SerialNumber,R.ReceiptDate as REDate,R.AcquisitionCost ,S.Condition,R.Room,R.Floor,R.AssetClassCode,
	HRCompany.Images,(select CompENG from SYHRCompany) CompENG,
	(CASE
		WHEN R.Condition='G' THEN 'Good'
	END) AS Condition,r.Description,r.StatusUse,S.Status,
	--(Select top 1 Description  from HRBranch where Branch=R.BranchCode) branch ,
	HRBranch.Description as Branch ,R.Qty,R.Description
from HRAssetStaff S
INNER JOIN HRAssetRegister R on S.AssetCode=R.AssetCode
INNER JOIN HRStaffProfile EMP ON S.EmpCode=EMP.EmpCode
INNER JOIN HRDepartment ON EMP.DEPT = HRDepartment.CODE 
INNER JOIN HRPosition  ON EMP.JobCode = HRPosition.Code
LEFT Join HRCompany ON HRCompany.Company=EMP.CompanyCode
INNER join HRBranch on EMP.Branch=HRBranch.Code and HRBranch.CompanyCode=EMP.CompanyCode 
LEFT OUTER JOIN HRLevel ON EMP.LevelCode= HRLevel.Code 
LEFT OUTER JOIN HRDivision ON EMP.Division= HRDivision.Code
LEFT OUTER JOIN HRLocation ON EMP.LOCT= HRLocation.Code
LEFT OUTER JOIN HRSection ON EMP.SECT= HRSection.Code
WHERE 
	(@Branch IS NULL OR @Branch='' OR  EMP.Branch=@Branch OR  EMP.Branch IN (SELECT VALUE FROM dbo.fn_Split(@Branch,','))) AND 
	(@Division IS NULL OR @Division='' OR  EMP.Division=@Division) AND 
	(@Department IS NULL OR @Department='' OR  EMP.DEPT=@Department) AND
	(@Section IS NULL OR @Section='' OR  EMP.Sect=@Section) AND 
	(@Position IS NULL OR @Position='' OR  EMP.JobCode=@Position) AND
	(@Level IS NULL OR @Level='' OR EMP.LevelCode=@Level) AND
	(S.ReturnDate between @FromDate AND @ToDate)</value>
  </data>
